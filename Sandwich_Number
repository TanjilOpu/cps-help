#include <bits/stdc++.h>
using namespace std;
#define int long long int
// #define endl '\n'
#define yes cout << "YES" << endl
#define no cout << "NO" << endl
#define nn cout << endl
#define vc vector<int>
#define pb push_back
#define ppb pop_back
#define f0(n) for (int i = 0; i < n; i++)
#define f1(n) for (int i = 1; i <= n; i++)
#define all(a) sort(a.begin(), a.end())
#define rall(a) sort(a.rbegin(), a.rend())
const int mod = 1e9 + 7;
#define debug(x) cerr << (#x) << " " << (x) << endl
#define fast()                            \
    {                                     \
        ios_base::sync_with_stdio(false); \
        cin.tie(NULL);                    \
    }

void solve()
{
    string s;
    cin >> s;
    int sz=s.length();
    if(sz!=8)
    {
        no;
        return;
    }
    char f,l;
    f=s[0];
    l=s[sz-1];
    if(!isupper(f))
    {
        no;
        return;
    }
    if(!isupper(l))
    {
        no;
        return;
    }
    string sn;
    sn=s.substr(1,6);
    cout<<"ns = "<<sn<<endl;

    if(sn[0]=='0')
    {
       no;
       return;
    }
    for(char it:sn)
    {
        if(!isdigit(it))

        {
            no;
            return;
        }
    }
    yes;
   
}

int32_t main()
{
    fast();
    int t = 1;
    //  cin >> t;
    while (t--)
        solve();
}
